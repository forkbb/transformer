msgstr "Veuillez attendre %s secondes et essayez d’envoyer à nouveau."

msgid "[%1$s] tag in blacklist"
msgstr "Marqueur [%1$s] en liste noire."

msgid "[%1$s] tag not in whitelist"
msgstr "Le marqueur[%1$s] n’est pas en liste blanche."

msgid "[%1$s] tag can't be opened in [%2$s] tag"
msgstr "Le marqueur [%1$s] ne peut être ouvert dans le marqueur[%2$s] ."

msgid "No start tag found for [/%1$s] tag"
msgstr "Pas de marqueur de départ trouvé pour le marqueur[/%1$s] ."

msgid "Found [/%1$s] tag for single [%1$s] tag"
msgstr "Trouvé marqueur [/%1$s] pour simple marqueur [%1$s] ."

msgid "No attributes in [%1$s] tag"
msgstr "Pas d’attributs dand l’étiquette [%1$s]."

msgid "Primary attribute is forbidden in [%1$s] tag"
msgstr "Le premier attribut est interdit dans le marqueur [%1$s]."

msgid "Secondary attribute is forbidden in [%1$s] tag"
msgstr "Le second attribut est interdit dans le marqueur [%1$s]."

msgid "Attribute '%2$s' does not match pattern in [%1$s] tag"
msgstr "L’attribut '%2$s' ne correspond pas au motif du marqueur [%1$s]."

msgid "[%1$s] tag contains unknown secondary attribute '%2$s'"
msgstr "Le marqueur [%1$s] contient un second attribut inconnu '%2$s'."

msgid "Body of [%1$s] tag does not match pattern"
msgstr "Le corps du marqueur [%1$s] ne correspond pas au motif."

msgid "[%1$s] tag was opened within itself, this is not allowed"
msgstr "Le marqueur [%1$s] esst ouvert dans lui même, ce qui est interdit."

msgid "Missing attribute '%2$s' in [%1$s] tag"
msgstr "Attribut manquant '%2$s' dans le marqueur [%1$s]."

