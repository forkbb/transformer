msgstr "Tous les marqueurs sont vides."

msgid "The depth of the tag tree is greater than %1$s"
msgstr "La profondeur de l’arbre de marqueurs est plus grand que %1$s."

msgid "[%1$s] tag is enclosed in itself more than %2$s times"
msgstr "Le marqueur [%1$s] est inclu en lui même plus que %2$s fois."

msgid "Invalid closing %1$s tag. Expected closing %2$s tag"
msgstr "Marqueur de fermeture invalide &lt;/ %1$s &gt; tag. Attendu le marqueur &lt;/ %2$s &gt; ."

msgid "%2$s attribute is not allowed in %1$s tag"
msgstr "L’attribut '%2$s' n'est pas autorisé dans le marqueur &lt; %1$s &gt; ."

msgid "Attempting to insert JavaScript into %2$s attribute of %1$s tag"
msgstr "Tentative d’insérer du  JavaScript dans l’attribut '%2$s' due marqueur &lt; %1$s &gt; ."

msgid "Invalid value for %2$s attribute [=%3$s] of %1$s tag"
msgstr "Valeur invalide pour le '%2$s' attribut [=%3$s] du marqueur &lt; %1$s &gt; ."

msgid "Missing required %2$s attribute in %1$s tag"
msgstr "Manque le '%2$s' attribut dans le marqueur &lt; %1$s &gt; ."

msgid "Invalid value for %2$s attribute of %1$s tag (combination)"
msgstr "Valeur invalide pour le '%2$s' attribut du marqueur &lt; %1$s &gt; (combinaison)."

msgid "Missing required %2$s attribute in %1$s tag (combination)"
msgstr "Manque le '%2$s' attribut dans le marqueur &lt; %1$s &gt; (combinaison)."

msgid "Not expected to close %1$s tag"
msgstr "Fermeture inattendue dans le marqueur &lt;/ %1$s &gt; ."

